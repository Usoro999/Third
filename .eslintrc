{
  "plugins": ["wdio"],
  "extends": [
      "eslint:recommended",
      "plugin:wdio/recommended", "eslint:recommended"
  ],
  "rules": {
    "no-unused-vars": ["error", { "vars": "none", "args": "after-used", "ignoreRestSiblings": false }]
},
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "mocha": true,
    "node": true
  }
}